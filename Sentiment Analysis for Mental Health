   "id": "c31f4e73-9259-4912-9641-e02847397262",
   "metadata": {},
   "outputs": [],
   "source": [
    "def remover_caracter_especial(texto):\n",
    "    rem = ''\n",
    "    for i in texto:\n",
    "        if i.isalnum():\n",
    "            rem = rem + i\n",
    "        else:\n",
    "            rem = rem + ' '  \n",
    "    return rem"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 19,
   "id": "b39afb1a-35a3-4a88-95a3-79953b912788",
   "metadata": {},
   "outputs": [],
   "source": [
    "def converter_minusculo(texto):\n",
    "    return texto.lower()"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 20,
   "id": "bc68cc45-84dc-45af-8ce6-c944eb40f83a",
   "metadata": {},
   "outputs": [],
   "source": [
    "def remover_stopwords(texto):\n",
    "    stop_words = set(stopwords.words('english'))\n",
    "    words = word_tokenize(str(texto))\n",
    "    return [w for w in words if w not in stop_words]"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 21,
   "id": "8455f00a-b4aa-496d-acf8-9970967d9257",
   "metadata": {},
   "outputs": [],
   "source": [
    "def stemmer(texto):\n",
    "    objeto_stemmer = SnowballStemmer('english')\n",
    "    return \" \".join([objeto_stemmer.stem(w) for w in texto])"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 22,
   "id": "568ce212-97d8-4355-b485-08fff5a9c828",
   "metadata": {},
   "outputs": [],
   "source": [
    "def processar_texto(texto):\n",
    "    texto = remover_caracter_especial(texto) \n",
    "    texto = converter_minusculo(texto)        \n",
    "    texto = remover_stopwords(texto)          \n",
    "    texto = stemmer(texto)                    \n",
    "    return texto"
   ]
  },
